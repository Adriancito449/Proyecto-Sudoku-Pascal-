Program Generador_Randon;

Function Gener(Sudoki:array of integer ):integer;

Var
	Fila: integer;
	columna: integer;
Begin

	for Fila:= 1 to 9 do
	Begin
		writeln();
		for columna:=1 to 9 do
		Begin
			Sudoki[Fila,columna]:= Sudoki[Fila,columna] + Random(9);
			write(Sudoki[Fila,columna]);

		End;
	End;

	End;


Var
Sudoki: array [9,9] of integer
Begin




	Gener(Sudoki);
	readln()


	End.





