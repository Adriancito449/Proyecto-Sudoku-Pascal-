program arreglitos;



var

revelador: array [1..9,1..9] of boolean;
i,j: integer;

begin


	revelador[1,2]:= true;2

	for i :=1 to 9 do
	begin

		writeln(' ');

		for j := 1 to 9 do
		begin
			
			write(revelador[i,j]);
			if j = 3 then
			begin
				write(' ');
			end;

			if j = 6 then
			begin
				write(' ');
			end;
		end;

		if i = 3 then
		begin
			writeln(' ');
		end;

		if i = 6 then
		begin
			writeln(' ');
		end;
	end;




	readln()
end.




